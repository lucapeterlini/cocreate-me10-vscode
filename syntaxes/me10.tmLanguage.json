{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "CoCreate me10",
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#numbers"
    },
    {
      "include": "#control-flow"
    },
    {
      "include": "#function-definitions"
    },
    {
      "include": "#math-functions"
    },
    {
      "include": "#drawing-commands"
    },
    {
      "include": "#system-commands"
    },
    {
      "include": "#options-qualifiers"
    },
    {
      "include": "#operators"
    },
    {
      "include": "#variables"
    }
  ],
  "repository": {
    "comments": {
      "name": "comment.block.me10",
      "begin": "\\{",
      "end": "\\}",
      "patterns": [
        {
          "name": "constant.character.escape.me10",
          "match": "\\\\."
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.single.me10",
          "begin": "'",
          "end": "'",
          "patterns": [
            {
              "name": "constant.character.escape.me10",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "string.quoted.single.me10",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.me10",
              "match": "\\\\."
            }
          ]
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.float.me10",
          "match": "(?i)\\b\\d+\\.\\d+([eE][+-]?\\d+)?\\b"
        },
        {
          "name": "constant.numeric.integer.me10",
          "match": "(?i)\\b\\d+\\b"
        },
        {
          "name": "constant.numeric.pi.me10",
          "match": "(?i)\\bPI\\b"
        }
      ]
    },
    "control-flow": {
      "name": "keyword.control.me10",
      "match": "(?i)\\b(IF|ELSE_IF|ELSE|END_IF|WHILE|END_WHILE|LOOP|EXIT_IF|END_LOOP|REPEAT|UNTIL|DEFINE|END_DEFINE|LOCAL|PARAMETER|RETURN|BREAK|CONTINUE|GO|SKIP|CATCH|ON_ERROR)\\b"
    },
    "function-definitions": {
      "name": "keyword.other.function.me10",
      "match": "(?i)\\b(DEFINE|END_DEFINE|LOCAL|PARAMETER|MACRO)\\b"
    },
    "math-functions": {
      "name": "support.function.math.me10",
      "match": "(?i)\\b(ABS|SIN|COS|TAN|ARCSIN|ARCCOS|ARCTAN|EXP|LN|LG|LOG|SQRT|SQR|SGN|FRAC|INT|RND|MAX|MIN|MOD|DIV|ROUND|TRUNC)\\b"
    },
    "drawing-commands": {
      "name": "support.function.drawing.me10",
      "match": "(?i)\\b(LINE|CIRCLE|ARC|POINT|RECTANGLE|POLYGON|SPLINE|BSPLINE|ELLIPSE|PARABOLA|DIMENSION|DIM_LINE|DIM_RADIUS|DIM_DIAMETER|DIM_ANGLE|HATCH|CHANGE_HATCH_DIST|TEXT|LEADER_LINE|CENTERLINE|MIRROR|ROTATE|SCALE|MOVE|COPY|DELETE|MODIFY|STRETCH|TRIM_ONE|TRIM_TWO|FILLET|CHAMFER|SPLIT|EQUIDISTANCE|CHANGE_LINETYPE|CHANGE_LINESIZE|CHANGE_COLOR|CHANGE_HATCH_COLOR|CHANGE_DIM_TEXTS|CHANGE_DIM_TEXT_ORIENTATION|CHANGE_DIM_OFFSET_LINE|CHANGE_DIM_OFFSET_PNT|CHANGE_DIM_ARROW|CHANGE_DIM_COLOR|CHANGE_DIM_TEXT_GAP|CHANGE_DIM_TEXT_SPACE|CHANGE_ELEM_INFO|CHANGE_GLOBAL_INFO|CHANGE_CURRENT_INFO|TRAP_ERROR|CHECK_ERROR|MEASURE_DISTANCE|EDIT_PART|PNT_XY|X_OF|Y_OF|READ|TABLE_STATUS|RE_SET_TAB|CONNECT_TABLE|CREATE_SUBPART|SMASH_SUBPART|SMASH_POLY|INQ_ENV|INQ|INQ_SELECTED_ELEM|ADD_ELEM_INFO)\\b"
    },
    "system-commands": {
      "name": "support.function.system.me10",
      "match": "(?i)\\b(LOAD|SAVE|STORE|LET|OPEN_INFILE|OPEN_OUTFILE|CLOSE_FILE|READ_FILE|WRITE_FILE|PLOT|PRINT_TABLE|NEW_SCREEN|REDRAW|DISPLAY|ZOOM|PAN|WINDOW|VIEW|SELECT|PRESELECT|ECHO|BEEP|WAIT|EXIT|RESET|UNDO|M_UNDO|M_REDO|CANCEL|END|SPOTLIGHT)\\b"
    },
    "options-qualifiers": {
      "name": "constant.language.me10",
      "match": "(?i)\\b(ON|OFF|YES|NO|TRUE|FALSE|ALL|NONE|CURRENT|GLOBAL|LOCAL|CONFIRM|CANCEL|HORIZONTAL|VERTICAL|PARALLEL|PERPENDICULAR|CENTER|TWO_PTS|THREE_PTS|ASSIST|AUTO|MANUAL|SOLID|DASHED|DOTTED|VISIBLE|INVISIBLE|FIRST|LAST|NEXT|BLACK|WHITE|RED|GREEN|BLUE|YELLOW|CYAN|MAGENTA|DEL_OLD|APPEND|KEEP_DATA|KEEP_DISPLAY|NO_SUBPARTS|SCREEN|BOX|POINT|IMMEDIATE|INTERACTIVE|LINESIZE|COMMAND|PNT|CIRCLES|LINES|ARCS|ADD|SUBTRACT)\\b"
    },
    "operators": {
      "patterns": [
        {
          "name": "keyword.operator.assignment.me10",
          "match": "=|:="
        },
        {
          "name": "keyword.operator.comparison.me10", 
          "match": "==|!=|<>|<=|>=|<|>"
        },
        {
          "name": "keyword.operator.logical.me10",
          "match": "(?i)\\b(AND|OR|NOT|EXOR)\\b"
        },
        {
          "name": "keyword.operator.arithmetic.me10",
          "match": "\\+|\\-|\\*|\\/|\\^"
        }
      ]
    },
    "variables": {
      "patterns": [
        {
          "name": "variable.other.me10",
          "match": "(?i)\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
        }
      ]
    }
  },
  "scopeName": "source.me10"
}